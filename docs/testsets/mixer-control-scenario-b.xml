<testset xmlns="http://cpee.org/ns/properties/2.0">
  <executionhandler>ruby</executionhandler>
  <dataelements/>
  <endpoints>
    <user>https-post://cpee.org/services/timeout-user.php</user>
    <auto>https-post://cpee.org/services/timeout-auto.php</auto>
    <subprocess>https-post://cpee.org/flow/start/url/</subprocess>
    <timeout>https://cpee.org/services/timeout.php</timeout>
    <send>https-post://cpee.org/ing/correlators/message/send/</send>
    <receive>https-get://cpee.org/ing/correlators/message/receive/</receive>
    <startmixer>https://lehre.bpm.in.tum.de/ports/40111/api/start</startmixer>
    <stopmixer>https://lehre.bpm.in.tum.de/ports/40111/api/stop</stopmixer>
    <powerstat>https://lehre.bpm.in.tum.de/ports/40111/api/power-statistics</powerstat>
    <wait>https://lehre.bpm.in.tum.de/ports/40111/api/wait</wait>
  </endpoints>
  <attributes>
    <guarded>none</guarded>
    <modeltype>CPEE</modeltype>
    <theme>extended</theme>
    <guarded_id/>
    <info>mixer-control</info>
  </attributes>
  <description>
    <description xmlns="http://cpee.org/ns/description/1.0">
      <_probability>
        <_probability_min/>
        <_probability_max/>
        <_probability_avg/>
      </_probability>
      <call id="a8" endpoint="startmixer">
        <parameters>
          <label>Start the Mixer</label>
          <method>:post</method>
          <arguments/>
        </parameters>
        <code>
          <prepare>data.startMixerError = false</prepare>
          <finalize output="result">data.energyConsumptionSinceStart = result['energyConsumptionSinceStart']
data.lastEnergyConsumptionToday = result['lastEnergyConsumptionToday']
data.startMixerError = false</finalize>
          <update output="result"/>
          <rescue output="result">data.startMixerError = result['error']</rescue>
        </code>
        <annotations>
          <_generic/>
          <_timing>
            <_timing_weight/>
            <_timing_avg/>
            <explanations/>
          </_timing>
          <_shifting>
            <_shifting_type>Duration</_shifting_type>
          </_shifting>
          <_context_data_analysis>
            <probes/>
            <ips/>
          </_context_data_analysis>
          <report>
            <url/>
          </report>
          <_notes>
            <_notes_general/>
          </_notes>
        </annotations>
        <documentation>
          <input/>
          <output/>
          <implementation>
            <description/>
          </implementation>
          <code>
            <description/>
          </code>
        </documentation>
      </call>
      <choose mode="exclusive">
        <alternative condition="!data.startMixerError">
          <manipulate id="a4" label="Start waiting">data.waiting = true</manipulate>
          <parallel wait="-1" cancel="last">
            <parallel_branch pass="" local="">
              <call id="a3" endpoint="wait">
                <parameters>
                  <label>Wait for duration</label>
                  <method>:get</method>
                  <arguments>
                    <duration>30</duration>
                  </arguments>
                </parameters>
                <code>
                  <prepare/>
                  <finalize output="result">data.waiting = false</finalize>
                  <update output="result"/>
                  <rescue output="result">data.waitingError = result['error']
data.waiting = false</rescue>
                </code>
                <annotations>
                  <_generic/>
                  <_timing>
                    <_timing_weight/>
                    <_timing_avg/>
                    <explanations/>
                  </_timing>
                  <_shifting>
                    <_shifting_type>Duration</_shifting_type>
                  </_shifting>
                  <_context_data_analysis>
                    <probes/>
                    <ips/>
                  </_context_data_analysis>
                  <report>
                    <url/>
                  </report>
                  <_notes>
                    <_notes_general/>
                  </_notes>
                </annotations>
                <documentation>
                  <input/>
                  <output/>
                  <implementation>
                    <description/>
                  </implementation>
                  <code>
                    <description/>
                  </code>
                </documentation>
              </call>
            </parallel_branch>
            <parallel_branch pass="" local="">
              <loop mode="pre_test" condition="data.waiting">
                <_probability>
                  <_probability_min/>
                  <_probability_max/>
                  <_probability_avg/>
                </_probability>
                <call id="a10" endpoint="timeout">
                  <parameters>
                    <label>Timeout 1 Second</label>
                    <method>:post</method>
                    <arguments>
                      <timeout>1</timeout>
                    </arguments>
                  </parameters>
                  <code>
                    <prepare/>
                    <finalize output="result"/>
                    <update output="result"/>
                    <rescue output="result"/>
                  </code>
                  <annotations>
                    <_generic/>
                    <_timing>
                      <_timing_weight/>
                      <_timing_avg/>
                      <explanations/>
                    </_timing>
                    <_shifting>
                      <_shifting_type>Duration</_shifting_type>
                    </_shifting>
                    <_context_data_analysis>
                      <probes/>
                      <ips/>
                    </_context_data_analysis>
                    <report>
                      <url/>
                    </report>
                    <_notes>
                      <_notes_general/>
                    </_notes>
                  </annotations>
                  <documentation>
                    <input/>
                    <output/>
                    <implementation>
                      <description/>
                    </implementation>
                    <code>
                      <description/>
                    </code>
                  </documentation>
                </call>
                <call id="a16" endpoint="powerstat">
                  <parameters>
                    <label>Get Power Stat</label>
                    <method>:get</method>
                    <arguments/>
                  </parameters>
                  <code>
                    <prepare/>
                    <finalize output="result">data.energyConsumptionSinceStart = result['energyConsumptionSinceStart']</finalize>
                    <update output="result"/>
                    <rescue output="result"/>
                  </code>
                  <annotations>
                    <_generic/>
                    <_timing>
                      <_timing_weight/>
                      <_timing_avg/>
                      <explanations/>
                    </_timing>
                    <_shifting>
                      <_shifting_type>Duration</_shifting_type>
                    </_shifting>
                    <_context_data_analysis>
                      <probes/>
                      <ips/>
                    </_context_data_analysis>
                    <report>
                      <url/>
                    </report>
                    <_notes>
                      <_notes_general/>
                    </_notes>
                  </annotations>
                  <documentation>
                    <input/>
                    <output/>
                    <implementation>
                      <description/>
                    </implementation>
                    <code>
                      <description/>
                    </code>
                  </documentation>
                </call>
              </loop>
            </parallel_branch>
          </parallel>
          <call id="a1" endpoint="stopmixer">
            <parameters>
              <label>Stop the Mixer</label>
              <method>:post</method>
              <arguments/>
            </parameters>
            <annotations>
              <_generic/>
              <_timing>
                <_timing_weight/>
                <_timing_avg/>
                <explanations/>
              </_timing>
              <_shifting>
                <_shifting_type>Duration</_shifting_type>
              </_shifting>
              <_context_data_analysis>
                <probes/>
                <ips/>
              </_context_data_analysis>
              <report>
                <url/>
              </report>
              <_notes>
                <_notes_general/>
              </_notes>
            </annotations>
            <documentation>
              <input/>
              <output/>
              <implementation>
                <description/>
              </implementation>
            </documentation>
          </call>
          <_probability>
            <_probability_min/>
            <_probability_max/>
            <_probability_avg/>
          </_probability>
        </alternative>
        <otherwise/>
      </choose>
    </description>
  </description>
  <transformation>
    <description type="copy"/>
    <dataelements type="none"/>
    <endpoints type="none"/>
  </transformation>
</testset>